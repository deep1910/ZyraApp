{
  "expo": {
    "name": "ZyraApp",
    "slug": "ZyraApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/src/assets/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
   
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/src/assets/icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "main":"./App.js"
  }
}
